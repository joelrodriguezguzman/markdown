{
  "name": "markdown",
  "displayName": "MarkDown",
  "description": "MD Editor",
  "version": "1.0.0",
  "icon": "images/logo128.png",
  "engines": {
    "vscode": "^1.102.0"
  },
 
  "repository": {
    "type": "git",
    "url": "https://github.com/joelrodriguezguzman/markdown.git"
    },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
        "activitybar": [
        {
            "id": "markdown",
            "title": "MD",
            "icon": "images/logo128.png"
        }
        ]
    },
    "views": {
        "markdown": [
        {
            "id": "markdown",
            "name": "Markdown",
            "icon": "images/logo128.png"
        }
        ]
    }
  },

  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.102.0",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.25.1",
    "typescript": "^5.8.3"
  }
}
